
@{
    ViewData["Title"] = "Import Files";
}
@model DisplayInputForm

<div class="menu-bar-main">
    <div class="menu-bar-title">
        File Import: 
    </div>
</div>

<p>
    Imported files will be automatically converted into texts into the selected binder.
</p>
    
<form enctype="multipart/form-data" method="post">
    <dl>
        <dt>
            <label asp-for="ImportEntry.File"></label>
        </dt>
        <dd>
            <input  asp-for="ImportEntry.File" multiple type="file">
        </dd>
    </dl>

    <input asp-page-handler="Upload" class="btn" type="submit" value="Upload">



</form>


